{
  "name": "gg",
  "displayName": "GeoGuessr Mute Emotes",
  "version": "1.0.4",
  "description": "Hide specified emotes when you are playing duels in GeoGuessr.",
  "author": "Posio",
  "scripts": {
    "check": "npm run lint && npx tsc",
    "dev": "plasmo dev",
    "build": "plasmo build",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "package": "plasmo package",
    "upgrade": "git pull && git fetch -p && npm ci && npm run check"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.10.0",
    "plasmo": "^0.87.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@types/chrome": "^0.0.270",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "eslint": "^8.47.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.3.3",
    "typescript": "^5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://www.geoguessr.com/*"
    ],
    "default_locale": "en"
  }
}
